<suite name="RegressionTestSuite" allow-return-values="true" thread-count="2" parallel="instances" verbose="10">
 	<listeners>
		<listener class-name="com.kotresh.demo.sele_demo.listeners.ExtentReportListener" />
		<listener class-name="com.kotresh.demo.sele_demo.listeners.TestAllureListener" />
	</listeners>
   <test name="ChromeTest">
      <parameter name="browser" value="Chrome" />
      <classes>
         <class name="com.kotresh.demo.sele_demo.mytests.LoginTest_TC1" />
      </classes>
   </test>
   <test name="FirefoxTest">
      <parameter name="browser" value="Firefox" />
      <classes>
         <class name="com.kotresh.demo.sele_demo.mytests.LoginTest_TC1" />
      </classes>
   </test>
   <test name="EdgeTest">
      <parameter name="browser" value="Edge" />
      <classes>
         <class name="com.kotresh.demo.sele_demo.mytests.LoginTest_TC1" />
      </classes>
   </test>
</suite>
